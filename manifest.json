{
  "name": "Bionic Reading",
  "description": "An extension that allows for Bionic Reading on any webpage!",
  "version": "0.0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "src/background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "src/popup.html"
  }
}